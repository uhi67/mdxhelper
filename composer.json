{
  "name": "uhi67/mdxhelper",
  "description": "Helper for SimpleSAMLphp mdx config",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "uhi67",
      "email": "uherkovich.peter@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "uhi67\\mdxhelper\\": "src"
    }
  },
  "require": {
    "ext-openssl": "*",
    "ext-json": "*",
    "simplesamlphp/simplesamlphp": "*"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.1",
    "uhi67/envhelper": "*"
  },
  "config": {
    "allow-plugins": {
      "simplesamlphp/composer-module-installer": true
    }
  }
}
